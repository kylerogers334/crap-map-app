{
  "name": "Multipleth",
  "version": "1.0.7",
  "description": "A data visualization map made with D3 and React.",
  "engines": { "node": "7.10.0" },
  "scripts": {
    "start": "node ./server/server.js",
    "dev": "concurrently \"nodemon ./server/server.js\" \"npm run watch\"",
    "webpack": "webpack --config ./config/webpack.config.js",
    "watch": "webpack --watch --config ./config/webpack.config.js",
    "lint": "eslint config/",
    "test": "concurrently \"npm run test:server\" \"npm run test:client\"",
    "test:client": "jest test/test-client",
    "test:server": "mocha --compilers js:babel-core/register test/test-server/test-server.js",
    "//": "bootstrap sometimes fails to install for some reason, added a redundancy.",
    "heroku-postbuild": "npm install bootstrap@4.0.0-alpha.6 && npm run webpack"
  },
  "author": "Kyle Rogers",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "eslint": "^4.6.0",
    "eslint-plugin-react": "^7.3.0",
    "fetch-mock": "^5.12.2",
    "jest": "^20.0.4",
    "redux-mock-store": "^1.2.3"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.6",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.5",
    "d3": "^4.10.0",
    "d3-scale-chromatic": "^1.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "jquery": "^3.2.1",
    "knex": "^0.13.0",
    "pg": "^7.3.0",
    "popper.js": "^1.12.5",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-transition-group": "^2.2.0",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.18.2",
    "topojson": "^3.0.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.3.0"
  }
}
