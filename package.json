{
	"name": "Multipleth",
	"version": "1.0.7",
	"description": "A data visualization map made with D3 and React",
	"engines": {
		"node": "10.x"
	},
	"scripts": {
		"start": "node ./server/server.js",
		"dev": "concurrently \"nodemon ./server/server.js\" \"npm run watch\"",
		"webpack": "webpack --config ./config/webpack.config.js",
		"watch": "webpack --watch --config ./config/webpack.config.js",
		"lint": "eslint config/",
		"test": "concurrently \"npm run test:server\" \"npm run test:client\"",
		"test:client": "jest test/test-client",
		"test:server": "mocha --compilers js:babel-core/register test/test-server/test-server.js"
	},
	"author": "Kyle Rogers",
	"license": "ISC",
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"babel-loader": "^8.0.6",
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"concurrently": "^4.1.0",
		"css-loader": "^3.0.0",
		"eslint": "^5.16.0",
		"eslint-plugin-react": "^7.13.0",
		"fetch-mock": "^7.3.3",
		"jest": "^24.8.0",
		"nodemon": "^1.19.1",
		"redux-mock-store": "^1.5.3",
		"style-loader": "^0.23.1",
		"webpack": "^4.35.0",
		"webpack-cli": "^3.3.4"
	},
	"dependencies": {
		"d3": "^5.9.2",
		"dotenv": "^8.0.0",
		"express": "^4.17.1",
		"knex": "^0.19.5",
		"pg": "^7.11.0",
		"popper.js": "*",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-redux": "^7.1.0",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0",
		"styled-components": "^4.3.2",
		"topojson": "^3.0.2"
	}
}
